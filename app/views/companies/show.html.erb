<%= @company.name %>
<%= @company.address %>
<%= @company.phone %>
<%= @company.description%>

<% if current_user.admin? %>
  <%= link_to "Edit", edit_company_path(@company) %>
  <%= link_to "Services", company_services_path(@company) %>
<% else %>
  <%= link_to "Services", company_services_path(@company) %>
<% end %>
